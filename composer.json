{
    "name": "nuxgame/laravel-dynamic-db-failover",
    "description": "A Laravel package to dynamically manage database failover between a primary and a failover connection, with a limited functionality mode.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Nuxgame\\LaravelDynamicDBFailover\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nuxgame\\LaravelDynamicDBFailover\\DynamicDBFailoverServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
