{
    "name": "nuxgame/laravel-dynamic-db-failover",
    "description": "Dynamically fails over database connections in Laravel.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michkire Dmytro",
            "email": "michkire@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/events": "^10.0|^11.0",
        "illuminate/console": "^10.0|^11.0",
        "illuminate/filesystem": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Nuxgame\\LaravelDynamicDBFailover\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nuxgame\\LaravelDynamicDBFailover\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "mockery/mockery": "^1.5",
        "orchestra/testbench": "^8.0|^9.0"
    }
}
